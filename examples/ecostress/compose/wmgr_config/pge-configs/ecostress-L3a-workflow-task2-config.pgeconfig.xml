<?xml version="1.0" encoding="UTF-8"?>
<pgeConfig>

	<!-- PGE commands -->
	<exe dir="[JobDir]" shell="/bin/bash">
		<cmd>cd [JobDir]</cmd>
                <cmd>export PATH=/usr/local/bin:$PATH</cmd>
                <cmd>python [OODT_HOME]/rabbitmq/rabbitmq_producer.py ecostress-L3b-workflow 1 Dataset=abc Project=123 heap=1 size=10 time=10</cmd>
	</exe>

	<customMetadata>
		<metadata key="ProductionDateTime" val="[DATE.UTC]" />
		<metadata key="JobDir" val="[PGE_ROOT]/ecostress/jobs/[WorkflowInstId]" />
	</customMetadata>

</pgeConfig>
